{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "author": "Pat",
    "manifest_version": 3,
    "name": "Night City Sky",
    "version": "1.0",
    "description": "Night city bg mady by yours truly",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "search",
        "activeTab"
    ],
    "icons": {
        "16": "img/128.png",
        "48": "img/128.png",
        "128": "img/128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}